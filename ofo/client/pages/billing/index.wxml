<!--pages/billing/index.wxml-->
<view class="container">
  <view class="number">
    当前单车编号：
    <text>{{number}}</text>
  </view>
  <view class="time">
    <view class="time-title">{{activeText}}</view>
    <view class="time-content">{{hours}} : {{minutes}} : {{seconds}} </view>
  </view>
  <view class="endride">
    <button type="warn" bindtap="endride" disabled="{{disabled}}">结束骑行</button>
    <button type="primary" bindtap="movetoindex">回到地图</button>
  </view>
</view>
